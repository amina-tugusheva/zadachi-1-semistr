//задача 3-6 №16
#include <iostream>
using namespace std;
float f(float x)
{
	float y;
	if (x > 0)
	{
		y = x;
		return y;
	}
	else if ((x >= -1) && (x <= 0))
	{
		y = 0;
		return y;
	}
	else
	{
		y = x * x + 1;
		return y;
	}
}
int main()
{
	float a, b;
	cin >> a >> b;
	float x, y, h;
	cin >> h;
	//x = a;
	for (x = a; x <= b; x += h)
	{
		//y = f(x);
		cout << "y = " << f(x) << endl;
	}
}
