//6-1 № 13 статический
#include <iostream>
using namespace std;
int main()
{
	const int n = 5, x = 2;

	int mas[n];
	for (int i = 0; i < n; i++)
	{
		cin >> mas[i];
	}
	for (int i = 0; i < n; i++)
	{
		if (mas[i] > x)
		{
			cout << i << " ";
		}
	}
	

}
