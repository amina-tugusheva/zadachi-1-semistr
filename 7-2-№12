// 7-2 â„–12
#include<iostream>
#include <string>
using namespace std;
int main()
{
	string s1, s2;
	cin >> s1 >> s2;
	int n1, n2;
	cin >> n1 >> n2;
	//cout << s1 + s2 << endl;
	s1.erase(n1, 10);
	//cout << s1 << endl;
	int k2 = s2.length();
	//cout << k2 << endl;
	string ss2 = s2.substr((k2 - n2), n2);
	string s = s1 + ss2;
	cout << s << endl;
	return 0;
}
